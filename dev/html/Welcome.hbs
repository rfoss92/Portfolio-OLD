{{> head}}
{{> navbar}}
<body>
	<div class="content">

	  <h1>Good Morning!</h1>

	  <section class="weather">
		  <div id="date"></div>	  	
		  <div id="time"></div>
		  <div id="description"></div>
		  <div id="temp"></div>
		  <div id="humidity"></div>
		  <div id="wind"></div>
	  </section>

		<div class="row">

			<section class="col-sm-6">
	    	<button type="button" class="btn btn-primary btn-lg" onclick="window.open('https://en.wikipedia.org/wiki/Special:Random');">Random Wikipedia Article</button>

	      <form>
	        <div class="input-group">
	    			<input id="searchTerm"  type="text" class="form-control" placeholder="Search...">
	    			<div class="input-group-btn">
		      		<button  id="search" class="btn btn-default" type="submit">
		        		<i class="glyphicon glyphicon-search"></i>
		      		</button>
	    			</div>
	  			</div>
	      </form>
	      <div id="output"></div>	      

			</section>	

			<section class="col-sm-6 quote">
			  <button type="button" class="btn btn-primary btn-lg" id=quoteButton>Get Quote!</button>
				<a id="tweetLink"><img src="https://cdn3.iconfinder.com/data/icons/social-icons-5/606/Twitter.png" /></a>		  
			  <p id="quote-content"></p>
			  <p id="quote-author"></p>
			</section>

		</div>
	</div>

<script src="/js/Welcome.js"></script>
</body>
{{> footer}}
